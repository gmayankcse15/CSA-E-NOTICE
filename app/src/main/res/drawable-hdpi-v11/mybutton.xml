<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android" >
    <item android:state_pressed="true" >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#123d40" />
            <gradient android:angle="-90" android:startColor="#b5cecf" android:endColor="#123d40"  />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#314844" />
            <solid android:color="#28504a"/>
        </shape>
    </item>
    <item >
        <shape android:shape="rectangle"  >
            <corners android:radius="3dip" />
            <stroke android:width="1dip" android:color="#123d40" />
            <gradient android:angle="-90" android:startColor="#a6bfbf" android:endColor="#123d40" />
        </shape>
    </item>
</selector>